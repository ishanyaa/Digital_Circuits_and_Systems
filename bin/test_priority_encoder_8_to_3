    %set/v v010C2D50_0, 8, 8;
    %delay 10, 0;
    %movi 8, 64, 8;
    %set/v v010C2D50_0, 8, 8;
    %delay 10, 0;
    %movi 8, 128, 8;
    %set/v v010C2D50_0, 8, 8;
    %delay 10, 0;
    %set/v v010C2D50_0, 1, 8;
