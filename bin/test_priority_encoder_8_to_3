#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_010C4DE0 .scope module, "test_priority_encoder_8_to_3" "test_priority_encoder_8_to_3" 2 50;
 .timescale 0 0;
v010C2D50_0 .var "A", 7 0;
v010C2DA8_0 .net "Y", 2 0, v010C2CA0_0; 1 drivers
v010CECF8_0 .net "valid", 0 0, v010C2CF8_0; 1 drivers
S_010C4E68 .scope module, "my_encoder" "priority_encoder_8_to_3" 2 56, 2 1, S_010C4DE0;
 .timescale 0 0;
v010CE778_0 .net "A", 7 0, v010C2D50_0; 1 drivers
v010C2CA0_0 .var "Y", 2 0;
v010C2CF8_0 .var "valid", 0 0;
E_010C8238 .event edge, v010CE778_0;
    .scope S_010C4E68;
T_0 ;
    %wait E_010C8238;
    %set/v v010C2CA0_0, 0, 3;
    %set/v v010C2CF8_0, 0, 1;
    %ix/load 1, 7, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.0, 4;
    %load/x1p 8, v010CE778_0, 1;
    %jmp T_0.1;
T_0.0 ;
    %mov 8, 2, 1;
T_0.1 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.2, 8;
    %set/v v010C2CA0_0, 1, 3;
    %set/v v010C2CF8_0, 1, 1;
    %jmp T_0.3;
T_0.2 ;
    %ix/load 1, 6, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.4, 4;
    %load/x1p 8, v010CE778_0, 1;
    %jmp T_0.5;
T_0.4 ;
    %mov 8, 2, 1;
T_0.5 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.6, 8;
    %movi 8, 6, 3;
    %set/v v010C2CA0_0, 8, 3;
    %set/v v010C2CF8_0, 1, 1;
    %jmp T_0.7;
T_0.6 ;
    %ix/load 1, 5, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.8, 4;
    %load/x1p 8, v010CE778_0, 1;
    %jmp T_0.9;
T_0.8 ;
    %mov 8, 2, 1;
T_0.9 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.10, 8;
    %movi 8, 5, 3;
    %set/v v010C2CA0_0, 8, 3;
    %set/v v010C2CF8_0, 1, 1;
    %jmp T_0.11;
T_0.10 ;
    %ix/load 1, 4, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.12, 4;
    %load/x1p 8, v010CE778_0, 1;
    %jmp T_0.13;
T_0.12 ;
    %mov 8, 2, 1;
T_0.13 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.14, 8;
    %movi 8, 4, 3;
    %set/v v010C2CA0_0, 8, 3;
    %set/v v010C2CF8_0, 1, 1;
    %jmp T_0.15;
T_0.14 ;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.16, 4;
    %load/x1p 8, v010CE778_0, 1;
    %jmp T_0.17;
T_0.16 ;
    %mov 8, 2, 1;
T_0.17 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.18, 8;
    %movi 8, 3, 3;
    %set/v v010C2CA0_0, 8, 3;
    %set/v v010C2CF8_0, 1, 1;
    %jmp T_0.19;
T_0.18 ;
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.20, 4;
    %load/x1p 8, v010CE778_0, 1;
    %jmp T_0.21;
T_0.20 ;
    %mov 8, 2, 1;
T_0.21 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.22, 8;
    %movi 8, 2, 3;
    %set/v v010C2CA0_0, 8, 3;
    %set/v v010C2CF8_0, 1, 1;
    %jmp T_0.23;
T_0.22 ;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.24, 4;
    %load/x1p 8, v010CE778_0, 1;
    %jmp T_0.25;
T_0.24 ;
    %mov 8, 2, 1;
T_0.25 ;
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.26, 8;
    %movi 8, 1, 3;
    %set/v v010C2CA0_0, 8, 3;
    %set/v v010C2CF8_0, 1, 1;
    %jmp T_0.27;
T_0.26 ;
    %load/v 8, v010CE778_0, 1; Only need 1 of 8 bits
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_0.28, 8;
    %set/v v010C2CA0_0, 0, 3;
    %set/v v010C2CF8_0, 1, 1;
T_0.28 ;
T_0.27 ;
T_0.23 ;
T_0.19 ;
T_0.15 ;
T_0.11 ;
T_0.7 ;
T_0.3 ;
    %jmp T_0;
