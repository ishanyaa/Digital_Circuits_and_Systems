#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01223898 .scope module, "tb_countor" "tb_countor" 2 18;
 .timescale 0 0;
v01062ED0_0 .var "clk", 0 0;
v01062F28_0 .net "count", 2 0, v01222D28_0; 1 drivers
v01062F80_0 .var "en", 0 0;
v01062FD8_0 .var "reset", 0 0;
S_01222CA0 .scope module, "uut" "countor" 2 22, 2 1, S_01223898;
 .timescale 0 0;
v01062C98_0 .net "clk", 0 0, v01062ED0_0; 1 drivers
v01222D28_0 .var "count", 2 0;
v01222D80_0 .net "en", 0 0, v01062F80_0; 1 drivers
v01062E78_0 .net "reset", 0 0, v01062FD8_0; 1 drivers
E_012241E8/0 .event negedge, v01062E78_0;
E_012241E8/1 .event posedge, v01062C98_0;
E_012241E8 .event/or E_012241E8/0, E_012241E8/1;
    .scope S_01222CA0;
T_0 ;
    %wait E_012241E8;
    %load/v 8, v01062E78_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 3, 0;
    %assign/v0 v01222D28_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v01222D80_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v01222D28_0, 3;
    %mov 11, 0, 1;
    %cmp/u 0, 8, 4;
    %jmp/0xz  T_0.4, 5;
    %load/v 8, v01222D28_0, 3;
    %mov 11, 0, 29;
    %subi 8, 1, 32;
    %ix/load 0, 3, 0;
    %assign/v0 v01222D28_0, 0, 8;
    %jmp T_0.5;
T_0.4 ;
    %ix/load 0, 3, 0;
    %assign/v0 v01222D28_0, 0, 1;
T_0.5 ;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_01223898;
T_1 ;
    %delay 5, 0;
    %load/v 8, v01062ED0_0, 1;
    %inv 8, 1;
    %set/v v01062ED0_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_01223898;
T_2 ;
    %set/v v01062ED0_0, 0, 1;
    %set/v v01062FD8_0, 0, 1;
    %set/v v01062F80_0, 0, 1;
    %vpi_call 2 29 "$dumpfile", "countor.vcd";
    %vpi_call 2 30 "$dumpvars", 1'sb0, S_01223898;
    %vpi_call 2 32 "$monitor", "Time = %t, count = %b", $time, v01062F28_0;
    %delay 10, 0;
    %set/v v01062FD8_0, 1, 1;
    %delay 10, 0;
    %set/v v01062FD8_0, 0, 1;
    %delay 10, 0;
    %set/v v01062F80_0, 1, 1;
    %delay 10, 0;
    %set/v v01062F80_0, 0, 1;
    %vpi_call 2 40 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
