    %jmp/1 T_0.5, 6;
    %cmpi/u 8, 6, 3;
    %jmp/1 T_0.6, 6;
    %cmpi/u 8, 7, 3;
    %jmp/1 T_0.7, 6;
    %set/v v011091C8_0, 0, 1;
    %jmp T_0.9;
T_0.0 ;
    %load/v 8, v01102C98_0, 1; Only need 1 of 8 bits
; Save base=8 wid=1 in lookaside.
