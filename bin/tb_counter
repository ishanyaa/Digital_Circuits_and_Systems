E_01213ED8/1 .event posedge, v01212CA0_0;
E_01213ED8 .event/or E_01213ED8/0, E_01213ED8/1;
    .scope S_01213898;
T_0 ;
    %wait E_01213ED8;
    %load/v 8, v01212DA8_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 3, 0;
    %assign/v0 v01212CF8_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v01212D50_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v01212CF8_0, 3;
    %mov 11, 0, 1;
    %cmp/u 0, 8, 4;
    %jmp/0xz  T_0.4, 5;
    %load/v 8, v01212CF8_0, 3;
    %mov 11, 0, 29;
    %subi 8, 1, 32;
    %ix/load 0, 3, 0;
    %assign/v0 v01212CF8_0, 0, 8;
    %jmp T_0.5;
T_0.4 ;
    %ix/load 0, 3, 0;
    %assign/v0 v01212CF8_0, 0, 1;
T_0.5 ;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_01213810;
T_1 ;
    %delay 5, 0;
    %load/v 8, v01063340_0, 1;
    %inv 8, 1;
    %set/v v01063340_0, 8, 1;
    %jmp T_1;
